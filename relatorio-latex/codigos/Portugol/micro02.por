programa
{
  funcao inicio ()
  {
    inteiro num1, num2
    escreva ( "Digite o primeiro numero: " )
    leia ( num1 )
    escreva ( "Digite o segundo numero: " )
    leia ( num2 )

    se ( num1 > num2 )
    {
      escreva ( "O primeiro numero ", num1, " eh maior que o segundo ", num2 )
    }
    senao
    {
      escreva ( "O segundo numero ", num2, " eh maior que o primeiro", num1 )
    }
  }
}
